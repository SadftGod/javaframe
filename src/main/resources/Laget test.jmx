        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">
String token = vars.get(&quot;JWT_TOKEN&quot;)

def tokenParts = token.split(&quot;\\.&quot;)

if (tokenParts.length == 3) {
    String payload = tokenParts[1]

    payload = payload.replace(&apos;-&apos;, &apos;+&apos;).replace(&apos;_&apos;, &apos;/&apos;)

    while (payload.length() % 4 != 0) {
        payload += &apos;=&apos;
    }

    byte[] decodedPayload = payload.getBytes(&apos;UTF-8&apos;)
    String decodedPayloadStr = new String(java.util.Base64.getDecoder().decode(decodedPayload))

    log.info(&quot;Decoded Payload: &quot; + decodedPayloadStr)

    def jsonSlurper = new groovy.json.JsonSlurper()
    def jsonPayload = jsonSlurper.parseText(decodedPayloadStr)

    def userId = jsonPayload.id 

    log.info(&quot;Extracted User ID: &quot; + userId)

    
    vars.put(&quot;user_id&quot;, userId.toString())  
} else {
    log.error(&quot;Invalid JWT token format&quot;)
}

//це код GPT я не знав ,як декодувати jwt
</stringProp>
